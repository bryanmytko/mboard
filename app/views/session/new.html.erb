<div id="signup">
<h1>Login</h1>
<%= form_tag session_index_path do %>
<div class="input_container clearfix">
	<%= label_tag :email %>
	<%= text_field_tag :email, params[:email] %>
</div>
<div class="input_container clearfix">
	<%= label_tag :password %>
	<%= password_field_tag :password %>
</div>
<div class="input_container clearfix">
	<%= submit_tag "Log In!" %>
</div>
<% end %>
</div>